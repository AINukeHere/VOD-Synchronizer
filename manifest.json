{
    "manifest_version": 3,
    "name": "VOD Synchronizer",
    "version": "0.0.9",
    "description":"SOOP, CHZZK 다시보기 타임스탬프 표시, 다른 스트리머의 다시보기와 동기화",
    "content_scripts": [
        {
            "matches": ["https://vod.sooplive.co.kr/*"],
            "js": ["timestamp_manager.js", "soop_timestamp_manager.js", "soop_content.js"],
            "all_frames": true
        },
        {
            "matches": ["https://ch.sooplive.co.kr/*"],
            "js": ["vod_get.js"],
            "all_frames": true
        },
        {
            "matches": ["https://www.sooplive.co.kr/*"],
            "js": ["StreamerIDManager.js"],
            "all_frames": true
        },
        {
            "matches": ["https://chzzk.naver.com/*"],
            "js": ["timestamp_manager.js", "chzzk_timestamp_manager.js", "chzzk_content.js"],
            "all_frames": true
        }
    ]
  }
  